{
	"expo": {
		"name": "Tasker",
		"slug": "tasker",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/iconStart.png",
		"splash": {
			"image": "./assets/background.png",
			"resizeMode": "contain",
			"backgroundColor": "#000"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"package":"tasker.pierslabs",
			"adaptiveIcon": {
				"foregroundImage": "./assets/iconStart.png",
				"backgroundColor": "#000"
			}
		},
		"web": {
			"favicon": "./assets/iconStart.png"
		}
	}
}
